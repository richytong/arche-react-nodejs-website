{
  "name": "arche-react-nodejs-website",
  "version": "0.0.0",
  "description": "Quickstart project for a dynamic website using Arche, React, and Node.js",
  "repository": {
    "type": "git",
    "url": "github.com/richytong/arche-react-nodejs-website"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "env": {
    "local": {
      "PORT": 4507,
      "HTML_SERVER_PORT": 4508,
      "HTML_SERVER_NO_CACHE": true,
      "BYPASS_PUBLIC_CACHE": true
    },
    "production": {
      "PORT": 8080,
      "HTML_SERVER_PORT": 8081,
      "HTML_SERVER_NO_CACHE": false,
      "BYPASS_PUBLIC_CACHE": false
    }
  }
}
